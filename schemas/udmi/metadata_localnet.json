{
  "title": "Local network metadata snippet",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "protocol": {
      "type": "string",
      "pattern": "^[a-z][a-z0-9]*(_[a-z0-9]+)*$"
    },
    "local_id": {
      "type": "string"
    }
  },
  "required": [
    "protocol",
    "local_id"
  ]
}
